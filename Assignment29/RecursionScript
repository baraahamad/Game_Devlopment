namespace Assignment29
{
using System.Collections;
using System.Collections.Generic;
using UnityEngine;
  public class RecursionScript : MonoBehaviour
    {
        int FibonacciRecursive(int n)
        {
            if (n <= 1) return n;
            return FibonacciRecursive(n - 1) + FibonacciRecursive(n - 2);
        }

        int FibonacciIterative(int n)
        {
            if (n <= 1) return n;

            int a = 0, b = 1, c = 0;
            for (int i = 2; i <= n; i++)
            {
                c = a + b;
                a = b;
                b = c;
            }
            return c;
        }

        void Start()
        {
            Debug.Log("Fibonacci Recursive (n=10): " + FibonacciRecursive(10));
            Debug.Log("Fibonacci Recursive (n=30): " + FibonacciRecursive(30));

            Debug.Log("Fibonacci Iterative (n=10): " + FibonacciIterative(10));
            Debug.Log("Fibonacci Iterative (n=30): " + FibonacciIterative(30));
        }
    }

}